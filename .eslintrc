{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "indent": ["error", 2],
    "no-tabs": 0,
    "no-mixed-spaces-and-tabs": 0,
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "linebreak-style": "off",
    "require-jsdoc": "off"
  }
}